<h3 class="admin-form-title">Sign In</h3>

<form action="/admin" method="POST">

  <label for="username" class="admin-form-label"><b>Username</b></label>
  <input type="text" placeholder="Enter Username" name="username" class="admin-form-input">

  <label for="password" class="admin-form-label"><b>Password</b></label>
  <input type="password" placeholder="Enter Password" name="password" class="admin-form-input">

  <input type="submit" value="Login" class="btn admin-form-btn">

</form>


